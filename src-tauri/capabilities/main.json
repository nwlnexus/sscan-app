{
  "identifier": "main-capability",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:app:default",
    "core:event:default",
    "core:image:default",
    "core:menu:default",
    "core:path:default",
    "core:resources:default",
    "core:tray:default",
    "core:webview:default",
    "core:window:default",
    "deep-link:allow-get-current",
    "deep-link:allow-is-registered",
    "deep-link:allow-register",
    "deep-link:allow-unregister",
    "deep-link:default",
    "fs:allow-write-file",
    "process:default",
    "shell:default",
    "sql:allow-close",
    "sql:allow-execute",
    "sql:allow-load",
    "sql:allow-select",
    "store:default"
  ]
}